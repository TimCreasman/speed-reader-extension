{
    "name": "SpeedReader",
    "description": "",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "js": ["content/speed-read.js"],
            "css": ["main.css"],
            "matches": ["*://*/*"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ]
}
